@model IEnumerable<BlogMvcApp.Models.Category>
<h2>KategoriListesi</h2>
<div class="row">
    <div class="list-group">
        @foreach (var item in Model)
        {

            <a href="/Blog/List/@item.Id" class="list-group-item list-group-item-action list-group-item-primary d-block ml-3 text-dark "><strong>@item.KategoriAdi</strong></a>

        }
    </div>
</div>
